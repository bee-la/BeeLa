        <activity
            android:name="br.ufrpe.beela.gui.usuario.AlterarSenha"
            android:label="@string/AlterarSenha" />
        <activity
            android:name="br.ufrpe.beela.gui.EsqueceuSenha"
            android:label="@string/esqueciSenha" />
        <activity
            android:name="br.ufrpe.beela.gui.perfil.Perfil"
            android:label="@string/perfil" />
        <activity
            android:name="br.ufrpe.beela.gui.perfil.PerguntasComidas"
            android:label="@string/adicionarPerfil" />
        <activity
            android:name="br.ufrpe.beela.gui.perfil.PerguntasMusica"
            android:label="@string/adicionarPerfil" />
        <activity
            android:name="br.ufrpe.beela.gui.perfil.NomePerfil"
            android:label="@string/nomePerfil"></activity>
    </application>

</manifest>